// Place your key bindings in this file to overwrite the defaults
[
    { "key": "ctrl+r", "command": "workbench.action.debug.restart", "when": "inDebugMode" },
    { "key": "alt+shift+s", "command": "workbench.action.showAllSymbols" },
    { "key": "alt+shift+o", "command": "workbench.action.quickOpen" },
    { "key": "alt+m", "command": "workbench.action.gotoSymbol" },
    { "key": "alt+g", "command": "editor.action.goToDeclaration", "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor" },
    { "key": "f7", "command": "workbench.action.tasks.runTask"}
]